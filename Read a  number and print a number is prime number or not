Read a number and print a number is prime number or not
#include <stdio.h>
#include<math.h>

int main()
{
    int i,n,c=0;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    
    {
       if(n%i==0)
           printf("Prime no");
       else
           printf("Not a prime no");
        
        
    }
        
        
             
        
        

        
        
    

    return 0;
}

Read three digit number and print number is amstrong number or not
#include <stdio.h>
#include<math.h>

int main()
{
    int n,d,s=0;
    scanf("%d",&n);
    t=n;
    while(n>0)
    {
        d=n%10;
        d=n/10;
        s=s+d*d*d;
    }
        if(s==0)
        
             printf("Amstrong no");
        
        else
        
             printf("Not an amstrong no");
        
        

        
        
    

    return 0;
}

read two numbers and print 1 for add and 2 for sub,3 for mult and 4 for div
#include <stdio.h>


int main()
{
   int a,b;
   scanf("%d%d",&a,&b);
   int ch;
   scanf("%d",&ch);
   switch(ch)
   {
       case1:
          printf("%d",a+b);
          break;
       case2:
          printf("%d",a-b);
          break;
       case3:
          printf("%d",a*b);
          break;
       case4:
          printf("%d",a/b);
          break;
      default:
          printf("Invalid choice");
          break;
   }
          
   
   
     
   
       
   
  
     
         
     
     
       
     
       
 
       
       
   
   

    
   
    return 0;
}

   
